                <INDEX NAME="IXFK_deadline_groups" UNIQUE="false" FIELDS="group_id"/>
                <INDEX NAME="IXFK_deadline_events" UNIQUE="false" FIELDS="event_id"/>
            </INDEXES>
        </TABLE>

        <TABLE NAME="charon_lab" COMMENT="Lab.">
            <FIELDS>
